{
  "name": "react-formik-step-wizard",
  "author": "kennyhei <kenny_heinonen@hotmail.com>",
  "description": "React multistep wizard with Formik integration",
  "homepage": "https://github.com/kennyhei/react-formik-step-wizard",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "module": "./dist/react-formik-step-wizard.es.js",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-formik-step-wizard.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kennyhei/react-formik-step-wizard"
  },
  "keywords": [
    "react",
    "formik",
    "step",
    "multistep",
    "wizard",
    "react-wizard",
    "formik-wizard",
    "step-wizard",
    "react-formik-step-wizard",
    "kennyhei"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "vite build -c vite.demo.config.js",
    "predeploy": "npm run build:demo",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "postpublish" : "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "dependencies": {
    "formik": "^2.4.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.7.3",
    "framer-motion": "^10.16.4",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.29",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-spinners": "^0.13.8",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
